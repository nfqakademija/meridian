<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <!-- BOOTSTRAP STYLES-->
        <link href="{{ asset('assets/css/bootstrap.css') }}" rel="stylesheet">

        <!-- FONTAWESOME STYLES-->
        <link href="{{ asset('assets/css/font-awesome.css') }}" rel="stylesheet">
        <!-- CUSTOM STYLES-->
        <link href="{{ asset('assets/css/custom.css') }}" rel="stylesheet">
        <!-- GOOGLE FONTS-->
        <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />

    {% endblock %}
</head>
<body>
<div id="wrapper">
    <nav class="navbar navbar-default navbar-cls-top " role="navigation" style="margin-bottom: 0">
        <div class="navbar-header">

            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" >Meridian</a>
        </div>
        {{ render(controller("MeridianCoreBundle:Proxy:getMaxScores")) }}
        <div style="color: #ffffff; text-align: center; vertical-align: middle;">
            Ar gali juos pralenkti? <a href="{{ path("fos_user_registration_register") }}">Užsiregistruok!</a>
        </div>

        <div style="color: white;
padding: 15px 50px 5px 50px;
float: right;
font-size: 16px;">
            <a href="{{ path("start_page") }}" class="btn btn-danger square-btn-adjust">Atsijungti</a> </div>
    </nav>
    <!-- /. NAV TOP  -->
    <nav class="navbar-default navbar-side" role="navigation">
        <div class="sidebar-collapse">
            <ul class="nav" id="main-menu">
                <li class="text-center">
                    <div class="name">
                        <h5> Demo </h5>
                    </div>
                    <img src="{{ asset('images/find_user.png') }}" alt="" class="user-image img-responsive" height="128" width="128">
                    {#<div class="edit-profile">#}
                        {#<a href="{{ path("fos_user_profile_edit") }}">Redaguoti profilį</a>#}
                    {#</div>#}
                </li>
                <li>
                    <a  href="{{ path("about") }}"><i class="fa fa-desktop fa-3x"></i> Apie projektą</a>
                </li>
            </ul>

        </div>

    </nav>
    <!-- /. NAV SIDE  -->
    <div id="page-wrapper" >
        <div id="page-inner">
            <div class="row">
                <div class="col-md-12">

                    {% block body %}

                        <h2>Blank Page</h2>
                        <h5>Welcome Jhon Deo , Love to see you back. </h5>
                    {% endblock %}

                </div>
            </div>
            <!-- /. ROW  -->
            <hr />

        </div>
        <!-- /. PAGE INNER  -->
    </div>
    <!-- /. PAGE WRAPPER  -->
</div>
<!-- /. WRAPPER  -->
<!-- SCRIPTS -AT THE BOTOM TO REDUCE THE LOAD TIME-->
<!-- JQUERY SCRIPTS -->
<script src="assets/js/jquery-1.10.2.js"></script>
<!-- BOOTSTRAP SCRIPTS -->
<script src="assets/js/bootstrap.min.js"></script>
<!-- METISMENU SCRIPTS -->
<script src="assets/js/jquery.metisMenu.js"></script>
<!-- CUSTOM SCRIPTS -->
<script src="assets/js/custom.js"></script>


</body>
</html>