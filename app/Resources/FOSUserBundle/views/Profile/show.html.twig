{% extends "::userbase.html.twig" %}

{% block body %}
    {% for type, messages in app.session.flashBag.all %}
        {% for message in messages %}
            <div class="{{ type }}">
                {{ message|trans({}, 'FOSUserBundle') }}
            </div>
        {% endfor %}
    {% endfor %}
    {% include "FOSUserBundle:Profile:show_content.html.twig" %}
{% endblock %}
